{
    "jshint.enable": false,
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "prettier.jsxSingleQuote": true,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 3,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 3
}
